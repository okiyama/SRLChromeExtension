{
  "manifest_version": 2,

  "name": "SpeedRunsLive Stream Viewer",
  "description": "For easily viewing the streams on SpeedRunsLive.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "images/icon19.png",
    "default_popup": "popup.html"
  },
  "icons": { 
  "48": "images/icon48.png",
  "128": "images/icon128.png"
  },
  "permissions": [
    "http://speedrunslive.com/", "http://www.twitch.tv/", "tabs"
  ]
}
